{
  "name": "sapphire-template",
  "version": "1.5.1",
  "private": true,
  "main": "dist/main.js",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install .github/husky",
    "build": "rimraf tsconfig.tsbuildinfo dist && yarn tsc --listEmittedFiles --incremental",
    "build:watch": "yarn tsc -w --incremental",
    "lint": "eslint src/ --ext .ts --cache",
    "lint:fix": "eslint src/ --fix --ext .ts --cache",
    "db:generate": "yarn prisma generate",
    "db:migrate": "doppler run yarn prisma migrate deploy || true",
    "start": "doppler run -- yarn node --enable-source-maps dist/main.js",
    "version:patch": "npm version patch -m \"chore: set version to %s\"",
    "version:minor": "npm version minor -m \"chore: set version to %s\"",
    "version:major": "npm version major -m \"chore: set version to %s\""
  },
  "dependencies": {
    "@bitomic/material-colors": "^1.0.0",
    "@discordjs/builders": "^1.6.3",
    "@discordjs/rest": "^1.7.1",
    "@prisma/client": "^4.16.1",
    "@sapphire/decorators": "^6.0.1",
    "@sapphire/discord-utilities": "^3.0.3",
    "@sapphire/duration": "^1.1.0",
    "@sapphire/framework": "4.4.4",
    "@sapphire/pieces": "^3.6.3",
    "@sapphire/plugin-api": "^5.1.0",
    "@sapphire/plugin-i18next": "^5.0.3",
    "@sapphire/plugin-logger": "^3.0.4",
    "@sapphire/plugin-scheduled-tasks": "^7.1.0",
    "@sapphire/plugin-utilities-store": "^1.0.1",
    "@sapphire/shapeshift": "^3.9.2",
    "@sapphire/utilities": "^3.12.0",
    "bullmq": "^3.15.8",
    "colorette": "^2.0.20",
    "discord-api-types": "^0.37.46",
    "discord.js": "^14.11.0",
    "fs-extra": "^11.1.1",
    "i18next": "^22.5.1",
    "imgur": "^2.3.0",
    "ioredis": "^5.3.2",
    "lodash": "^4.17.21",
    "pino": "^8.14.1",
    "pino-pretty": "^10.0.0",
    "prisma": "^4.16.1",
    "reflect-metadata": "^0.1.13",
    "ts-dotenv": "^0.9.1",
    "undici": "^5.22.1"
  },
  "devDependencies": {
    "@bitomic/eslint-config": "^1.2.0",
    "@types/fs-extra": "^11.0.1",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.3.2",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "eslint": "^8.43.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "rimraf": "^4.4.1",
    "typescript": "^4.9.5"
  },
  "packageManager": "yarn@3.3.1"
}
