{
  "name": "sapphire-template",
  "version": "2.7.1",
  "private": true,
  "main": "dist/main.js",
  "license": "MIT",
  "type": "module",
  "imports": {
    "#decorators/*": "./dist/decorators/*.js",
    "#drizzle/*": "./dist/drizzle/*.js",
    "#framework/*": "./dist/framework/*.js",
    "#lib/*": "./dist/lib/*.js",
    "#utils/*": "./dist/utils/*.js"
  },
  "scripts": {
    "prepare": "husky install .github/husky",
    "build": "rimraf tsconfig.tsbuildinfo dist && yarn tsc --listEmittedFiles --incremental",
    "build:watch": "yarn tsc -w --incremental",
    "lint": "eslint src/ --ext .ts --cache",
    "lint:fix": "eslint src/ --fix --ext .ts --cache",
    "db:migrate": "doppler run yarn drizzle-kit push:mysql || true",
    "start": "doppler run -- yarn node --enable-source-maps dist/main.js",
    "version:patch": "npm version patch -m \"chore: set version to %s\"",
    "version:minor": "npm version minor -m \"chore: set version to %s\"",
    "version:major": "npm version major -m \"chore: set version to %s\""
  },
  "dependencies": {
    "@bitomic/material-colors": "^1.1.0",
    "@discordjs/builders": "^1.7.0",
    "@discordjs/rest": "^2.0.1",
    "@quority/core": "^4.0.4",
    "@quority/fandom": "^4.0.9",
    "@sapphire/decorators": "^6.0.2",
    "@sapphire/discord-utilities": "^3.1.1",
    "@sapphire/duration": "^1.1.0",
    "@sapphire/framework": "4.7.2",
    "@sapphire/pieces": "^3.7.0",
    "@sapphire/plugin-api": "^5.1.2",
    "@sapphire/plugin-i18next": "^5.1.1",
    "@sapphire/plugin-logger": "^3.0.6",
    "@sapphire/plugin-scheduled-tasks": "^7.1.2",
    "@sapphire/plugin-utilities-store": "^1.0.2",
    "@sapphire/shapeshift": "^3.9.3",
    "@sapphire/utilities": "^3.13.0",
    "bullmq": "^4.13.2",
    "colorette": "^2.0.20",
    "discord-api-types": "^0.37.63",
    "discord.js": "^14.13.0",
    "drizzle-kit": "^0.20.2",
    "drizzle-orm": "^0.29.0",
    "fs-extra": "^11.1.1",
    "i18next": "^22.5.1",
    "imgur": "^2.3.0",
    "ioredis": "^5.3.2",
    "lodash": "^4.17.21",
    "mysql2": "^3.6.3",
    "pino": "^8.16.1",
    "pino-pretty": "^10.2.3",
    "reflect-metadata": "^0.1.13",
    "ts-dotenv": "^0.9.1",
    "ts-pattern": "^5.0.5",
    "undici": "^5.27.2"
  },
  "devDependencies": {
    "@bitomic/eslint-config": "^1.3.0",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash": "^4.14.201",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.53.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.2.2"
  },
  "packageManager": "yarn@3.6.4"
}
