    "@sapphire/plugin-api": "^4.0.1",
    "@sapphire/utilities": "^3.8.0",
    "colorette": "^2.0.19",
    "discord-api-types": "^0.37.28",
    "discord.js": "^13.8.1",
    "fs-extra": "^10.1.0",
    "ioredis": "^5.2.4",
    "mysql2": "^2.3.3",
    "pino": "^8.8.0",
    "pino-pretty": "^9.1.1",
    "sequelize": "^6.28.0",
    "ts-dotenv": "^0.9.0"
  },
  "devDependencies": {
    "@bitomic/eslint-config": "^1.0.0",
